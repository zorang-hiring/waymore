{
    "name": "zorang/interview-waymore",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Features\\": "features/"
        }
    },
    "require": {
        "php": "^7.2",
        "vlucas/phpdotenv": "^5.4",
        "psr/log": "^1.1",
        "guzzle/guzzle": "^3.8.1",
        "monolog/monolog": "^2.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "acceptance": "./vendor/bin/behat"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "behat/behat": "^3.7"
    },
    "config": {
        "platform":{
            "php":"7.2"
        }
    }
}
